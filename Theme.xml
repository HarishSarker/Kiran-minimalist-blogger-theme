    <b:skin><![CDATA[
      body { margin: 0; padding: 0; background: #ffffff; font-family: 'Roboto', sans-serif; color: #202124; -webkit-font-smoothing: antialiased; }
      .container { max-width: 800px; margin: 0 auto; padding: 0 20px; }
      
      a { text-decoration: none !important; color: inherit; transition: color 0.2s ease; }
      a:hover { color: #1a73e8 !important; }

      /* Pure Text Header Styling */
      header/* Signature Header Style */
header { 
  padding: 60px 0 20px 0; 
  text-align: center; 
  background: #ffffff; 
}

.header-text h1 { 
  font-family: 'Pacifico', cursive; /* The handwritten font */
  font-size: 64px;                 /* Large signature size */
  font-weight: 400; 
  margin: 0; 
  color: #000000;                  /* Solid black like the image */
  letter-spacing: 0; 
  text-transform: none;            /* Keep it natural */
}

.header-text h1 a { 
  color: #000000; 
  text-decoration: none; 
}
      .divider, .footer-divider { border-top: 1px solid #f1f3f4; margin: 10px 0 40px 0; }

      .post { margin-bottom: 40px; }
      .post-title h2 { font-size: 28px; font-weight: 400; margin: 0; }
      .post-date { font-size: 14px; color: #70757a; margin: 5px 0 20px 0; display: block; }
      .post-body { font-size: 18px; line-height: 1.7; color: #3c4043; }
      .post-body img { max-width: 100%; height: auto; border-radius: 4px; }

      

      .blog-pager { margin: 50px 0; display: flex; justify-content: center; gap: 20px; }
      .blog-pager a { border: 1px solid #e8eaed; padding: 8px 25px; border-radius: 20px; font-size: 14px; color: #1a73e8; font-weight: 500; }
      .blog-pager a:hover { background: #f8f9fa; }

      .comments-area { margin-top: 50px; padding-top: 30px; border-top: 1px solid #f1f3f4; }
      .comment-list { list-style: none; padding: 0; margin-bottom: 30px; }
      .comment-item { padding: 15px 0; border-bottom: 1px solid #f8f9fa; }

      footer { margin-top: 60px; padding: 40px 0; text-align: center; color: #70757a; font-size: 13px; }
      .footer-menu { list-style: none; padding: 0; margin: 0 0 15px 0; display: flex; justify-content: center; gap: 25px; }

      @media screen and (min-width: 768px) {
        .header-text h1 { font-size: 48px; }
        .header-description { font-size: 18px; }
      }
    ]]></b:skin>
